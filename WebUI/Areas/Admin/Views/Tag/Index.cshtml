@model List<Tag>
<a asp-area="Admin" asp-controller="Tag" asp-action="Create" class="btn btn-dark w-100">Create</a>
<table class="table table-striped table-hover table-dark">
    <thead>
        <tr>
            <th>#</th>
            <th>Tag Name</th>
            <th>#</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.TagName</td>
                <td>
                    <a href="/admin/tag/Update/@item.Id" class="btn btn-warning">Edit</a>
                    <a href="/admin/tag/delete/@item.Id" class="btn btn-danger">Delete</a>
                    <a href="/admin/tag/detail/@item.Id" class="btn btn-success">Detail</a>
                </td>
            </tr>
        }
    </tbody>
</table>