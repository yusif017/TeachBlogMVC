@model IEnumerable<Category>

<a href="/admin/category/Create" class="btn btn-dark">Create</a>
<table class="table table-dark">
    <thead>
        <tr>
            <th>#</th>
            <th>Category Name</th>
            <th>#</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.CategoryName</td>
                <td>
                    <a href="/admin/category/edit" class="btn btn-warning">Edit</a>
                    <a href="/admin/category/delete" class="btn btn-danger">Delete</a>
                    <a href="/admin/category/detail" class="btn btn-info">Detail</a>
                </td>
            </tr>
        }
    </tbody>
</table>